<bootstrap id="DT-TEST">
    <environments>
        <variable key="HTTP_STACK" value="http"/>
        <variable key="RPC_STACK" value="rpc"/>
        <variable key="HTTP_PORT" value="8083"/>
    </environments>
    <centralizedService class="org.helium.framework.route.center.ZkCentralizedService">
        <setters>
            <setter field="zkHosts">192.168.156.16:7998</setter>
        </setters>
    </centralizedService>
    <stacks>
        <stack id="${HTTP_STACK}" class="org.helium.http.servlet.HttpServletStack">
            <setters>
                <setter field="host">${LOCAL_IP}</setter>
                <setter field="port">${AUTO_PORT}</setter>
                <setter field="sslPort">0</setter>
            </setters>
        </stack>
        <stack id="${RPC_STACK}" class="org.helium.framework.rpc.RpcServerStack">
            <setters>
                <setter field="host">${LOCAL_IP}</setter>
                <setter field="port">${AUTO_PORT}</setter>
            </setters>
        </stack>
    </stacks>
    <beans>
        <bean class="org.helium.redis.RedisDedicatedTagManager"/>
        <bean class="org.helium.dtask.tester.SampleDedicatedTask" export="true"/>
        <bean class="org.helium.dtask.tester.TaskLaunchServiceImpl" export="true"/>
    </beans>
    <bundles>
        <!-- dashboard -->
        <bundle path="bundle.xml"/>
    </bundles>
</bootstrap>