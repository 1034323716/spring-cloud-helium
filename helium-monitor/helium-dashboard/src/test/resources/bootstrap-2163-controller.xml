<bootstrap id="DT-TEST-CNTL">
    <environments>
        <variable key="HTTP_STACK" value="http"/>
        <variable key="RPC_STACK" value="rpc"/>
        <variable key="RPC_PORT" value="6700"/>
        <variable key="HTTP_PORT" value="8700"/>
    </environments>
    <centralizedService class="org.helium.framework.route.center.ZkCentralizedService">
        <setters>
            <setter field="zkHosts">192.168.156.16:7998</setter>
        </setters>
    </centralizedService>
    <stacks>
        <stack id="${RPC_STACK}" class="org.helium.framework.rpc.RpcServerStack">
            <setters>
                <setter field="host">${LOCAL_IP}</setter>
                <setter field="port">${RPC_PORT}</setter>
            </setters>
        </stack>
    </stacks>
    <beans>
        <bean class="org.helium.dtask.tester.ControllerBootstrap" export="true"/>
    </beans>
</bootstrap>