<bootstrap id="core-node">
    <environments imports="">
        <variable key="PRIVATE_IP" value="${LOCAL_IP}"/>
        <variable key="RPC_URL" value="${PRIVATE_IP}:${RPC_PORT}"/>
        <variable key="RPC_PORT" value="7026"/>
    </environments>
    <centralizedService class="org.helium.framework.route.center.ZkCentralizedService">
        <setters>
            <setter field="zkHosts">10.10.220.91:7998</setter>
        </setters>
    </centralizedService>
    <stacks>
        <stack id="rpc" class="org.helium.framework.rpc.RpcServerStack">
            <setters>
                <setter field="host">${PRIVATE_IP}</setter>
                <setter field="port">${RPC_PORT}</setter>
            </setters>
        </stack>
    </stacks>

  <bundles>
    <bundle path="bundle.xml">
      <grayFactors condition="or"  priority="50" duplicate="true">
        <factor key="mobile" operator="regex" value=".*13601030000.*"/>
      </grayFactors>
    </bundle>
  </bundles>
</bootstrap>