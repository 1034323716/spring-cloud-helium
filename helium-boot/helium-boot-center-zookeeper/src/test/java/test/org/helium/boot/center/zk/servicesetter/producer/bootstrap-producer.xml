<bootstrap id="bootstrap-producer">
    <environments imports="">
        <variable key="PRIVATE_IP" value="127.0.0.1"/>
        <variable key="RPC_URL" value="127.0.0.1:10002"/>
        <variable key="RPC_PORT" value="10002"/>
        <variable key="ZK_HOST" value="127.0.0.1:7998"/>
    </environments>
    <centralizedService class="org.helium.framework.route.zk.ZkCentralizedService">
        <setters>
            <setter field="zkHosts">127.0.0.1:7998</setter>
        </setters>
    </centralizedService>
    <stacks>
        <stack id="rpc" class="org.helium.stack.rpc.RpcServerStack">
            <setters>
              <setter field="host">127.0.0.1</setter>
              <setter field="port">10002</setter>
            </setters>
        </stack>
    </stacks>

    <beans>
      <bean class="test.org.helium.boot.center.zk.servicesetter.producer.SimpleProducerServiceImpl" export="true"/>
    </beans>
</bootstrap>