<bootstrap id="bootstrap-consumer">
    <environments imports="">
        <variable key="PRIVATE_IP" value="127.0.0.1"/>
        <variable key="RPC_PORT" value="10001"/>
        <variable key="RPC_URL" value="127.0.0.1:10001"/>
       <variable key="ZK_HOST" value="127.0.0.1:7998"/>
    </environments>
    <centralizedService class="org.helium.framework.route.zk.ZkCentralizedService">
      <setters>
        <setter field="zkHosts">${ZK_HOST}</setter>
      </setters>
    </centralizedService>
    <stacks>
        <stack id="rpc" class="org.helium.stack.rpc.RpcServerStack">
            <setters>
                <setter field="host">${PRIVATE_IP}</setter>
                <setter field="port">${RPC_PORT}</setter>
            </setters>
        </stack>
    </stacks>
  <beans>
    <bean class="test.org.helium.boot.center.zk.servicesetter.consumer.SimpleConsumerServiceImpl" export="true"/>
  </beans>
</bootstrap>